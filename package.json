{
  "name": "@jstoolkit/web-utils",
  "version": "1.34.1",
  "description": "Web utils",
  "author": "VZH",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/js-toolkit/web-utils"
  },
  "scripts": {
    "clean": "rimraf dist/*",
    "copy:configs": "copyfiles package.json .npmignore README.md LICENSE ./dist/",
    "minify": "node-utils-minify --replace ./dist",
    "build": "npm run clean && tsc --build ./tsconfig.json && npm run minify",
    "build-to-tutoron": "tsc --build ./tsconfig.json && copyfiles -u 1 './dist/**/*' '../../../tutoron/node_modules/@jstoolkit/web-utils'",
    "build-to-player": "tsc --build ./tsconfig.json && npm run minify && copyfiles -e './dist/*(package.json|README.md|LICENSE)' -u 1 './dist/**/*' '../../../player/modules/web-utils'",
    "build-to-react-hooks": "tsc --build ./tsconfig.json && npm run minify && copyfiles -u 1 './dist/**/*' '../react-hooks/node_modules/@jstoolkit/web-utils'",
    "build-to-player-utils": "tsc --build ./tsconfig.json && npm run minify && copyfiles -u 1 './dist/**/*' '../player-utils/node_modules/@jstoolkit/web-utils'",
    "build-to-styled-components": "tsc --build ./tsconfig.json && npm run minify && copyfiles -u 1 './dist/**/*' '../styled-components/node_modules/@jstoolkit/web-utils'",
    "patch-publish": "npm run build && npm version patch --no-workspaces-update -m 'v%s' && npm run copy:configs && cd ./dist && npm publish --access public && git push origin master --follow-tags",
    "minor-publish": "npm run build && npm version minor --no-workspaces-update -m 'v%s' && npm run copy:configs && cd ./dist && npm publish --access public && git push origin master --follow-tags"
  },
  "optionalDependencies": {
    "@js-toolkit/node-utils": "^1.1.4"
  },
  "devDependencies": {
    "@jstoolkit/configs": "^3.84.7",
    "@jstoolkit/utils": "^1.45.6",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.27.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eventemitter3": "^4.0.7",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "terser": "^5.15.1",
    "typescript": "^4.8.4",
    "webpack": "^5.74.0",
    "yargs": "^17.6.2"
  }
}
